<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
<!--    Inheriting SelectPartnerButton template for adding the button disable feature-->
    <t name="pos_access_right_hr.SelectPartnerButton" owl="1"
       t-inherit="point_of_sale.SelectPartnerButton" t-inherit-mode="extension">
        <xpath expr="//button[hasclass('set-partner')]" position="attributes">
            <attribute name="t-att-disabled">disable_customer()</attribute>
        </xpath>
    </t>
</templates>
